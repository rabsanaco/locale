{
  "name": "rabsanaco/locale",
  "description": "Locale management and direction detection based on laravel default and fallback locale",
  "keywords": ["laravel", "locale", "dir"],
  "homepage": "https://github.com/rabsanaco/locale",
  "license": "MIT",
  "authors": [
    {
      "name": "Sajjad Sisakhti",
      "email": "sajjad.30sakhti@gamil.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/config": "~5.0",
    "illuminate/support": "~5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Rabsanaco\\Locale\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rabsanaco\\Locale\\ServiceProvider"
      ],
      "aliases": {
        "Loc": "Rabsanaco\\Locale\\LocaleFacade"
      }
    }
  },
  "minimum-stability": "dev"
}
